<div
  style="padding-right: 50px; padding-left: 50px; font-size: 10px; font-family: 'Verdana', serif;"
>{{! Title }}
  <p style="text-align: center; font-weight: 400; font-size: 18px;">Bill Of
    Loading</p>
  {{! only space not a value }}
  <p style="text-align: center;">&nbsp;</p>
  <table
    style="border-collapse: collapse; float: right;"
    border="1"
    cellspacing="0"
    cellpadding="0"
  >
    <tbody>
      <tr>
        <td>
          <p style="margin-left: 5px; font-weight: 400;">Load Number</p>
        </td>
        <td style="width: 155.273px; height: 23px;">
          <p style="margin-left: 5px; font-weight: 400;">{{dispatcherDetails.loadNumber}}</p>
        </td>
      </tr>
      {{!-- <tr style="height: 35px;">
        <td style="width: 204.727px; height: 35px;">
          <p style="margin-left: 5px; font-weight: 400;">BOL Number</p>
        </td>
        <td style="width: 155.273px; height: 35px;">
          <p style="margin-left: 5px; font-weight: 400;">101</p>
        </td>
      </tr> --}}
      <tr style="height: 35px;">
        <td style="width: 204.727px; height: 35px;">
          <p style="margin-left: 5px; font-weight: 400;">Ship Date</p>
        </td>
        <td style="width: 155.273px; height: 35px;">
          <p style="margin-left: 5px; font-weight: 400;">{{dispatcherDetails.shipDate}}</p>
        </td>
      </tr>
      <tr style="height: 35px;">
        <td style="width: 204.727px; height: 35px;">
          <p style="margin-left: 5px; font-weight: 400;">Delivery Date</p>
        </td>
        <td style="width: 155.273px; height: 35px;">
          <p style="margin-left: 5px; font-weight: 400;">{{dispatcherDetails.delDate}}</p>
        </td>
      </tr>
      <tr style="height: 35px;">
        <td style="width: 204.727px; height: 35px;">
          <p style="margin-left: 5px; font-weight: 400;">P.O.Number</p>
        </td>
        <td style="width: 155.273px; height: 35px;">
          <p style="margin-left: 5px; font-weight: 400;">{{dispatcherDetails.PO}}</p>
        </td>
      </tr>
      <tr style="height: 35px;">
        <td style="width: 204.727px; height: 35px;">
          <p style="margin-left: 5px; font-weight: 400;">Freight Charges</p>
        </td>
        <td style="width: 155.273px; height: 35px;">
          <p style="margin-left: 5px; font-weight: 400;">{{dispatcherDetails.freightCharge}}</p>
        </td>
      </tr>
    </tbody>
  </table>
  <p><strong>{{companyDetails.company}}</strong></p>
  <p style="font-weight: 400;">{{companyDetails.address}}</p>
  <p style="font-weight: 400;">{{companyDetails.country}}</p>
  <p style="font-weight: 400;">Tel: {{companyDetails.primaryNumber}}</p>
  {{! only space not a value }}
  <p style="text-align: center;"><br /><br /><br /><br /></p>
  <p style="text-align: center;">&nbsp;</p>
  <p style="text-align: center;">&nbsp;</p>
  <p style="text-align: center;">&nbsp;</p>
  <p style="text-align: center;">&nbsp;</p>
</div>
